<Window x:Class="Processing.net.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ed="http://icsharpcode.net/sharpdevelop/avalonedit"
        Title="p55.net" Height="350" Width="525">
    <DockPanel>
        <Menu Height="Auto" DockPanel.Dock="Top">
            <MenuItem Header="_File"></MenuItem>
            <MenuItem Header="_Program">
                <MenuItem Header="_Run" Click="MenuItem_Click" />
                <MenuItem Header="_Parse" Click="Parse_Click" />
            </MenuItem>
        </Menu>
        <ed:TextEditor Name="_editor" ShowLineNumbers="True">
        </ed:TextEditor>
    </DockPanel>
</Window>
